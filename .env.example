# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_USER=your_github_username

# GitBack Configuration
GITBACK_OUTPUT_DIR=./backups
GITBACK_THREADS=5
GITBACK_TIMEOUT=30
GITBACK_INCLUDE_GISTS=true
GITBACK_NO_AUTH=false

# Logging Configuration
GITBACK_LOG_LEVEL=info  # debug, info, warn, error
GITBACK_LOG_FORMAT=text  # text or json
